    "@types/ioredis": "^4.16.2",
    "@types/lodash": "^4.14.153",
    "@types/luxon": "^1.24.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.12",
    "@types/shortid": "0.0.29",
    "@types/sinon": "^9.0.3",
    "@types/socket.io": "^2.1.7",
    "@types/socket.io-redis": "^1.0.26",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
